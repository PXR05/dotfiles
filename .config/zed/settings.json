{
  "icon_theme": "Colored Zed Icons Theme Dark",
  "features": {
    "edit_prediction_provider": "zed"
  },
  "telemetry": {
    "metrics": false,
    "diagnostics": true
  },
  "base_keymap": "VSCode",
  "ui_font_size": 18,
  "buffer_font_size": 20,
  "buffer_font_family": "JetBrainsMono Nerd Font Mono",
  "format_on_save": "prettier",
  "autosave": "on_window_change",
  "soft_wrap": "editor_width",
  "theme": {
    "mode": "dark",
    "light": "Serendipity Midnight Minimal",
    "dark": "Tokyo Night"
  },
  "git_panel": {
    "button": true,
    "dock": "right"
  },
  "toolbar": {
    "breadcrumbs": false,
    "selections_menu": false,
    "quick_actions": false
  },
  "tab_bar": {
    "show_nav_history_buttons": false
  },
  "assistant": {
    "button": false,
    "enabled": false,
    "version": "2"
  },
  "outline_panel": {
    "dock": "right",
    "button": false
  },
  "collaboration_panel": {
    "button": false
  },
  "chat_panel": {
    "button": "never"
  },
  "terminal": {
    "dock": "bottom",
    "font_size": 18
  },
  "project_panel": {
    "dock": "right"
  }
}
